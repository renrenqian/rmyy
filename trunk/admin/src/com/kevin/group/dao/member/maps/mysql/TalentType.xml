<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="TalentType">

    <!-- list the OPSer info -->
    <select id="list"  parameterClass="TalentType"  resultClass="TalentType">
     <![CDATA[
     select 
        TG_ID as tgId,
        TG_TYPE as tgType,
        TG_NAME as tgName,
        TG_CODE as tgCode
       from TALENT_GENRE
     ]]>
    </select>
     
</sqlMap>