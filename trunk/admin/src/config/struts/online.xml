<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="group" namespace="/group" extends="defaultj">
        <!-- Consultation start -->
        <!-- new Consultation -->
        <action name="addConsultation" method="addConsultation" class="ConsultationInfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- list Consultation -->
       <action name="listConsultation" method="listConsultation" class="ConsultationInfoAction">
            <result name="success" type="json">
               <param name="ignoreHierarchy">false</param>
               <param name="excludeNullProperties">true</param>
            </result>
       </action>
        <!-- search Consultation -->
       <action name="searchConsultation" method="searchConsultation" class="ConsultationInfoAction">
            <result name="success" type="json">
               <param name="ignoreHierarchy">false</param>
               <param name="excludeNullProperties">true</param>
            </result>
       </action>
        <!-- update the Consultation info -->
        <action name="updateConsultation" method="updateConsultation" class="ConsultationInfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- delete the Consultation -->
        <action name="deleteConsultation" method="deleteConsultation" class="ConsultationInfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- batch delete the Consultation -->
        <action name="batchDeleteConsultation" method="batchDeleteConsultation" class="ConsultationInfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- Consultation end -->
    </package>
</struts>