<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="online" namespace="/online" extends="defaultj">
        <!-- Consultation start -->
        <!-- new Consultation -->
        <action name="addConsultation" method="addConsultation" class="consultationfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- list Consultation -->
       <action name="listConsultation" method="listConsultation" class="consultationfoAction">
            <result name="success" type="json">
               <param name="ignoreHierarchy">false</param>
               <param name="excludeNullProperties">true</param>
            </result>
       </action>
        <!-- search Consultation -->
       <action name="searchConsultation" method="searchConsultation" class="consultationfoAction">
            <result name="success" type="json">
               <param name="ignoreHierarchy">false</param>
               <param name="excludeNullProperties">true</param>
            </result>
       </action>
        <!-- update the Consultation info -->
        <action name="updateConsultation" method="updateConsultation" class="consultationfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- delete the Consultation -->
        <action name="deleteConsultation" method="deleteConsultation" class="consultationfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- batch delete the Consultation -->
        <action name="batchDeleteConsultation" method="batchDeleteConsultation" class="consultationfoAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- Consultation end -->
        
        <!-- Employee start -->
        <!-- new Employee -->
        <action name="addEmployee" method="addEmployee" class="empAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- list Employee -->
       <action name="listEmployee" method="listEmployee" class="empAction">
            <result name="success" type="json">
               <param name="ignoreHierarchy">false</param>
               <param name="excludeNullProperties">true</param>
            </result>
       </action>
        <!-- search Employee -->
       <action name="searchEmployee" method="searchEmployee" class="empAction">
            <result name="success" type="json">
               <param name="ignoreHierarchy">false</param>
               <param name="excludeNullProperties">true</param>
            </result>
       </action>
        <!-- update the Employee info -->
        <action name="updateEmployee" method="updateEmployee" class="empAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- delete the Employee -->
        <action name="deleteEmployee" method="deleteEmployee" class="empAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- batch delete the Employee -->
        <action name="batchDeleteEmployee" method="batchDeleteEmployee" class="empAction">
            <result name="success" type="json">
                <param name="ignoreHierarchy">false</param>
                <param name="excludeNullProperties">true</param>
            </result>
        </action>
        <!-- Employee end -->
    </package>
</struts>